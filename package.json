{
  "name": "km-coin-api",
  "version": "1.0.0",
  "description": "Coin api",
  "author": "Khang Huynh <khanghuynh92@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=km-coin-api:* yarn start",
    "build": "gulp",
    "precommit": "yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:khanghuynh92/km-coin-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "2.5.1",
    "helmet": "3.12.0",
    "http-status": "^1.0.1",
    "joi": "13.2.0",
    "jsonwebtoken": "8.2.1",
    "method-override": "^2.3.5",
    "mongoose": "5.0.16",
    "morgan": "1.9.0",
    "node-fetch": "2.1.2",
    "winston": "2.4.2",
    "synaptic": "^1.0.8"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "^4.1.2",
    "commitizen": "^2.9.2",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "cz-conventional-changelog": "2.1.0",
    "del": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "husky": "^0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.1.1",
    "run-sequence": "^2.2.1",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "airbnb"
    ],
    "plugins": [
      "transform-runtime",
      "transform-regenerator"
    ]
  }
}
